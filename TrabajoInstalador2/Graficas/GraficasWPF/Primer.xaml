<Window x:Class="GraficasWPF.Primer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GraficasWPF"
        mc:Ignorable="d"
        Title="Primer" Height="450" Width="800">
    <Window.Background>
        <ImageBrush ImageSource="Imagenes/Fondos-de-deportes.jpg"/>
    </Window.Background>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <WrapPanel Grid.Row="0">
           
        </WrapPanel>
        <WrapPanel Grid.Row="1">
            <Button Width="300" Height="50" x:Name="TablaResultado" Click="TablaResultado_Click">Tablas de resultado</Button>
        </WrapPanel>
        <WrapPanel Grid.Row="2">
            <Button Width="300" Height="50" x:Name="GraficaResultado" Click="GraficaResultado_Click">Graficas de resultados</Button>
        </WrapPanel>
        <WrapPanel Grid.Row="3">
            <Button Width="300" Height="50" x:Name="btnTablaResultadoGeberal" Click="btnTablaResultadoGeberal_Click">Tabla de resultados general</Button>
        </WrapPanel>
    </Grid>
</Window>
